{
    "editor.tabSize": 4,
    "editor.rulers": [
        80
    ],
    "files.exclude": {
        "**/__pycache__": true
    },
    "editor.formatOnSave": true,
    "files.trimFinalNewlines": true,
    "files.insertFinalNewline": true,
    "cSpell.words": [
        "combatible",
        "ctypes",
        "pyreadline",
        "pytest",
        "readline",
        "venv",
        "virtualenv"
    ]
}
