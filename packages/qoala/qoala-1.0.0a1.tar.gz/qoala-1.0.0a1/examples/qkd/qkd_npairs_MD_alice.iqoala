  callback: 
  return_vars: m<{N}>
  remote_id: {bob_id}
  epr_socket_id: 0
  num_pairs: {N}
  virt_ids: all 0
  timeout: 1000
  fidelity: 1.0
  typ: measure_directly
  role: create
