META_START
    name: alice
    parameters: bob_id, N
    csockets: 0 -> bob
    epr_sockets: 0 -> bob
META_END

^b1 {type = QC}:
    outcomes<N> = run_request() : req

^b2 {type = CL}:
    return_result(outcomes)


REQUEST req
