(void o) start (int ME_rank) {
    string script_file = "%s/%s" % (emews_root, "{{cookiecutter.script_file}}");
    location me_loc = locationFromRank(ME_rank);
    EQR_init_script(me_loc, script_file) =>
    EQR_get(me_loc) =>
    EQR_put(me_loc, me_config_file) =>
      loop(me_loc) => {
        EQR_stop(me_loc) =>
        EQR_delete_R(me_loc);
        o = propagate();
    }
}