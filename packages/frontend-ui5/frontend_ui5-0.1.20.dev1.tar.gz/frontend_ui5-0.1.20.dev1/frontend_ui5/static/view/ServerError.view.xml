<mvc:View
  xmlns:core="sap.ui.core"
  controllerName="demo.spa.controller.ServerError"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc"
>
  <MessagePage
    title="{i18n>serverError.title}"
    titleLevel="H1"
    text="{i18n>serverError.text}"
    description="{i18n>serverError.description}"
    icon="sap-icon://error"
    showNavButton="true"
    navButtonPress="onNavBack"
  />
</mvc:View>
