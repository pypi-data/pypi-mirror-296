<%!
import cpsar.controls as C
show_notifications = False
%>
<%inherit file="page.tmpl" />
<%block name="head">
  <title>Manual Entry</title>
  <link rel='stylesheet' type='text/css'
        href='/css/redmond/jquery-ui-1.8.14.custom.css' />
  <link type='text/css' rel='stylesheet'
        href='/css/form.css'  />
  <link rel='stylesheet' type='text/css' href='/css/state_reporting.css' />
</%block>
<%def name="scripts()">
    <script src='/js/jquery-1.5.1.min.js'></script>
    <script src='/js/jquery.jgrowl.js'></script>
    <script src='/js/jquery.blockUI.js'></script>
    <script src='/js/jquery.numeric.js'></script>
    <script src='/js/jquery-ui-1.8.14.custom.min.js'></script>
    <script src='/repo/js/kcontrol/currency_control.js'></script>
    <script src='/js/common.js'></script>
    <script type='text/javascript'>
        $(document).ready(function() { });
    </script>
</%def>
<%block name="header">
   <a href="/state_reporting">State Reporting</a> &rarr; Manual Entry
</%block>
<%def name='lbl(id, cap)'>
    <label for='${id}'>${cap}</label>
</%def>
<form action='/sr_manual/add' method='post'>
<% C.update_store(args) %>
<% C.update_store(bill) %>
<div class='content_header'>
  <table cellspacing='0' cellpadding='0' border='0' class='content_header'>
  <tr>
    <td>
        ${C.ListBox('status_flag', values = [
          ('T', 'T - Test'),
          ('P', 'P - Production')
        ])}
    </td>
    <td>
      <label for='file_name'>File Name</label>
      ${C.TextBox('file_name')}
    </td>

    <td>
      <input type='submit' value='Save' /></td>
  </tr>
  </table>
</div>

  ${self.notifications()}
  <%include file="sr_bill_form.tmpl" />

</form>
