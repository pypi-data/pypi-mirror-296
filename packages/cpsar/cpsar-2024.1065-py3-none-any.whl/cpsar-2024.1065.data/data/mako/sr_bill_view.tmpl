% for i, entry in enumerate(bill['lines']):
  <% line_no = i + 1 %>
  <table class='form'>
  <tr>
    <td class='entry-caption'>${line_no}.</td>
    <td class='entry-caption'>Date of Service</td>
    <td class='entry-value'>${entry['date_of_service']}</td>
    <td class='entry-caption'>CPT Proc/HCPCS</td>
    <td class='entry-value'>${entry['hcpcs']}</td>
    <td class='entry-caption'>CPT Proc/HCPCS Paid</td>
    <td class='entry-value'>${entry['hcpcs_paid']}</td>
    <td class='entry-caption' title='5-4-2'>NDC</td>
    <td class='entry-value'>${entry['ndc' ]}</td>
    <td class='entry-caption'>Drug name</td>
    <td class='entry-value'>${entry['drug_name' ]}</td>
    <td class='entry-caption'>Day's Supply</td>
    <td class='entry-value'>${entry['days_supply' ]}</td>
  </tr>
  <tr>
    <td></td>
    <td class='entry-caption'>Line Charge</td>
    <td class='entry-value'>${entry['total' ]}</td>
    <td class='entry-caption'>Sales Tax</td>
    <td class='entry-value'>${entry['sales_tax' ]}</td>
    <td class='entry-caption'>State Fee</td>
    <td class='entry-value'>${entry['state_fee' ]}</td>
    <td class='entry-caption'>Insurer Paid</td>
    <td class='entry-value'>${entry['insurer_paid' ]}</td>
    <td class='entry-caption'>Repackaged NDC</td>
    <td class='entry-value'>${entry['repackaged_ndc']}</td>
  </tr>
  <tr>
    <td></td>
    <td class='entry-caption'>Rx #</td>
    <td class='entry-value'>${entry['rx_number' ]}</td>
    <td class='entry-caption'>Refill #</td>
    <td class='entry-value'>${entry['refill_number' ]}</td>
    <td class='entry-caption'>Quantity</td>
    <td class='entry-value'>${entry['quantity' ]}</td>
    <td class='entry-caption'>EOB Review Code</td>
    <td class='entry-value'>${entry['eob_review_code' ]}</td>
    <td class='entry-caption'>DAW</td>
    <td class='entry-value'>${entry['daw' ]}</td>
  </tr>
  <tr>
    <td></td>
    <td class='entry-caption'>Doc. Last Name</td>
    <td class='entry-value'>${entry['entry_doctor_last_name']}</td>
    <td class='entry-caption'>First Name</td>
    <td class='entry-value'>${entry['entry_doctor_first_name']}</td>
    <td class='entry-caption'>State License #</td>
    <td class='entry-value'>${entry['entry_doctor_state_lic_number']}</td>
    <td class='entry-caption'>NPI</td>
    <td class='entry-value'>${entry['entry_doctor_npi']}</td>
  </tr>
  <tr>
    <td></td>
    <td class='entry-caption'>Trans ID</td>
    <td class='entry-value'>${entry['trans_id' ]}</td>
    <td class='entry-caption'>SRID</td>
    <td class='entry-value'>${entry['srid']}</td>
  </tr>
  </table>
% endfor
