{
    "cSpell.customDictionaries": {
        "custom": true,
        "internal-terms": true,
        "project-words": {
            "addWords": true,
            "description": "Words that Max uses a lot.",
            "name": "Max's Dictionary",
            "path": "$HOME/maxludden/.config/.cspell/maxDictionary.txt"
        }
        "python": true,
        "custom-dictionary-user": {
            "name": "custom-dictionary-user",
            "path": "$HOME/maxludden/.config/.cspell/maxDictionary.txt",
            "addWords": true,
            "scope": "user"
        }
    },
    "cSpell.userWords": [
        "colour",
        "configalias",
        "craigslist",
        "greylock",
        "maxbook",
