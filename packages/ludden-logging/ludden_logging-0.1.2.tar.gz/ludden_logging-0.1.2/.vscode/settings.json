    "editor": {
        "fontFamily": "\"MesloLGS NF\", monospace",
        "fontVariations": true,
        "tabCompletion": "on",
        "renderFinalNewline": "off",
        "quickSuggestions": {
            "comments": "on",
            "strings": "inline"
        },
        "snippetSuggestions": "bottom",
        "trimAutoWhitespace": true,
        "suggest.preview": true,
        "inlineSuggest.enabled": true,
        "bracketPairColorization.enabled": true,
        "bracketPairColorization.independentColorPoolPerBracketType": true,
        "formatOnPaste": true,
        "formatOnSave": true,
        "formatOnType": true,
        "defaultFormatter": "ms-python.black-formatter",
        "codeActionsOnSave": {
            "source.organizeImports": "explicit",
            "source.unusedImports": "explicit",
            "source.fixAll": "explicit",
            "editor.action.formatDocument": "explicit"
        },
        "tokenColorCustomizations": {
            "[One Dark Drop]": {
                "textMateRules": [
                    {
                        "scope": "support.type.property-name.json.comments",
                        "settings": {
                            "foreground": "#9f00ff"
                        }
                    },
                    {
                        "scope": [
                            "punctuation.support.type.property-name.begin.json.comments",
                            "punctuation.support.type.property-name.end.json.comments"
                        ],
                        "settings": {
                            "foreground": "#8279A7ff"
                        }
                    },
                    {
                        "name": "quotations",
                        "scope": [
                            "punctuation.definition.string.begin.python",
                            "punctuation.definition.string.end.python",
                            "punctuation.definition.string.begin.json",
                            "punctuation.definition.string.end.json",
                            "punctuation.definition.string.begin.markdown",
                            "punctuation.definition.string.end.markdown"
                        ],
                        "settings": {
                            "foreground": "#797566",
                            "fontStyle": "bold"
                        }
                    },
                    {
                        "scope": "constant.language.json.comments",
                        "settings": {
                            "foreground": "#ff00af"
                        }
                    },
                    {
                        "scope": "constant",
                        "settings": {
                            "foreground": "#ffaaff"
                        }
                    },
                    {
                        "scope": "entity.name.function",
                        "settings": {
                            "foreground": "#5fccff"
                        }
                    },
                    {
                        "scope": "entity.name.type",
                        "settings": {
                            "foreground": "#fffb00",
                            "fontStyle": "italic"
                        }
                    },
                    {
                        "scope": "entity.name.class",
                        "settings": {
                            "foreground": "#633ef7",
                            "fontStyle": "italic"
                        }
                    },
                    {
                        "name": "",
                        "scope": [
                            "punctuation.definition.group.begin",
                            "punctuation.definition.group.end"
                        ],
                        "settings": {
                            "foreground": "#FF0000"
                        }
                    }
                ]
            }
        },
        "colorDecorators": true,
        "colorDecoratorsLimit": 800
    },
    "terminal": {
        "integrated": {
            "fontFamily": "MesloLGS NF",
            "copyOnSelection": true,
            "defaultProfile.osx": "zsh",
            "profiles.osx": {
                "bash": {
                    "path": "bash",
                    "args": [
                        "-l"
                    ],
                    "icon": "terminal-bash"
                },
                "zsh": {
                    "path": "zsh",
                    "args": [
                        "-l"
                    ]
                },
                "fish": {
                    "path": "fish",
                    "args": [
                        "-l"
                    ]
                },
                "tmux": {
                    "path": "tmux",
                    "icon": "terminal-tmux"
                },
                "pwsh": {
                    "path": "pwsh",
                    "icon": "terminal-powershell"
                }
            },
            "bellDuration": 10000,
            "cursorWidth": 1000,
            "scrollback": 10000,
            "persistentSessionScrollback": 20000,
            "rightClickBehavior": "paste",
            "enableImages": true,
            "fontSize": 13,
            "letterSpacing": 1,
            "smoothScrolling": true,
            "cursorBlinking": true
        }
    },
    "files": {
        "autoSave": "afterDelay",
        "defaultLanguage": ".py",
        "insertFinalNewline": true,
        "trimTrailingWhitespace": true,
        "autoGuessEncoding": true,
        "simpleDialog.enable": true
    },
    "workbench": {
        "editorAssociations": {
            "*.html": "default",
            "*.jinja": "default",
            "git-rebase-todo": "default",
            "*.sqlite": "default"
        },
        "colorTheme": "One Dark Drop",
        "preferredLightColorTheme": "One Dark Drop",
        "preferredDarkColorTheme": "One Dark Drop",
        "preferredHighContrastColorTheme": "One Dark Drop",
        "preferredHighContrastLightColorTheme": "Default Light Modern",
        "editor.autoLockGroups": {
